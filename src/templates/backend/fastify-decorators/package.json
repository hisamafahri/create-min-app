{
  "name": "<%= projectName %>",
  "version": "0.1.0",
  "main": "src/index.ts",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "nodemon -r dotenv/config src/start.ts",
    "debug": "nodemon --inspect -r dotenv/config src/start.ts",
    "start": "node -r dotenv/config dist/src/start.js",
    "build": "tsc",
    "production": "pnpm db:migration:run && tsc && pnpm start",
    "clean": "rm -rf node_modules dist .turbo",
    "db:migration:generate": "NODE_OPTIONS='-r dotenv/config' dotenv_config_path='.env' typeorm-ts-node-commonjs migration:generate ./src/migrations/migrations -d ./src/configs/datasources/postgresql.datasource.ts",
    "db:migration:run": "NODE_OPTIONS='-r dotenv/config' dotenv_config_path='.env' typeorm-ts-node-commonjs migration:run --dataSource src/configs/datasources/postgresql.datasource.ts",
    "db:migration:revert": "NODE_OPTIONS='-r dotenv/config' dotenv_config_path='.env' typeorm-ts-node-commonjs migration:revert --dataSource src/configs/datasources/postgresql.datasource.ts",
    "db:migration:show": "NODE_OPTIONS='-r dotenv/config' dotenv_config_path='.env' typeorm-ts-node-commonjs migration:show --dataSource src/configs/datasources/postgresql.datasource.ts"
  },
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/cookie": "^9.0.4",
    "@fastify/cors": "^8.3.0",
    "@fastify/error": "^3.3.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/sensible": "^5.2.0",
    "@fastify/type-provider-typebox": "^3.5.0",
    "@sinclair/typebox": "^0.31.8",
    "bullmq": "^4.8.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "envalid": "^7.3.1",
    "fastify": "^4.22.1",
    "fastify-decorators": "^3.15.1",
    "fastify-plugin": "^4.5.1",
    "figlet": "^1.6.0",
    "ioredis": "^5.3.2",
    "ms": "^2.1.3",
    "node-fetch": "^2.6.1",
    "node-os-utils": "^1.3.7",
    "pg": "^8.11.3",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.6",
    "@types/ms": "^0.7.31",
    "@types/node": "^20.5.7",
    "@types/node-fetch": "^2.6.4",
    "@types/node-os-utils": "^1.3.1",
    "@types/qrcode": "^1.5.2",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
